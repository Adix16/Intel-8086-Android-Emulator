    <dimen name="item_reg_margin">6dp</dimen>

    <style name="MyEmulatorTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/gray_0</item>
        <item name="colorSecondary">@color/gray_2</item>
    </style>

    <style name="MyEmulatorTheme2" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/gray_0</item>
        <item name="colorSecondary">@color/gray_2</item>
    </style>

    <style name="MyTextViewStyle" parent="@android:style/TextAppearance.Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">36dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="MyFrameLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">16dp</item>
        <item name="android:layout_marginBottom">4dp</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:background">@drawable/bg_main_options</item>
        <item name="android:foreground">@drawable/ripple</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
    </style>
    <style name="MyEmulatorTextView" parent="@android:style/TextAppearance.Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/pink</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="MyRegisterItemKeyStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/gray_1</item>
        <item name="android:fontFamily">@font/source_code_pro_regular</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="MyRegisterItemValueStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/bg_indiv_register</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:layout_weight">2</item>
        <item name="android:textColor">@color/gray_1</item>
        <item name="android:fontFamily">@font/source_code_pro_regular</item>
        <item name="android:textSize">16sp</item>
    </style>
</resources>